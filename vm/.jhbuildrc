# -*- mode: python -*-
# -*- coding: utf-8 -*-

# if you have a GNOME git account, uncomment this line
# repos['git.gnome.org'] = 'ssh://user@git.gnome.org/git/'

# what module set should be used.
moduleset = [
    os.path.expanduser('~/eos-sdk.modules'),
    'gnome-suites-core-3.10'
]

# A list of the modules to build.
modules = [ 'endless-apps' ]

# what directory should the source be checked out to?
checkoutroot = os.path.expanduser('~/checkout')

# the prefix to configure/install modules to (must have write access)
prefix = os.path.expanduser('~/install')

# custom CFLAGS / environment pieces for the build
os.environ['CFLAGS'] = '-g -O0'

#autogenargs=''
module_autogenargs = {}
module_autogenargs['eos-sdk'] = '--enable-gtk-doc --enable-gir-doc --enable-strict-flags'

# A string listing additional arguments to be passed to make.
#makeargs = ''
